<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="0dp" android:right="0dp" android:bottom="0dp" android:top="0dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="0.5dp"
                android:color="@color/black"/>
            <corners
                android:bottomLeftRadius="7dp"
                android:bottomRightRadius="7dp"
                android:topLeftRadius="7dp"
                android:topRightRadius="7dp" />
        </shape>
    </item>
    <item android:left="0.5dp" android:right="0.5dp" android:bottom="0.5dp" android:top="0.5dp">
        <shape
            android:shape="rectangle">
            <stroke android:width="2.5dp"
                android:color="@color/white"/>
            <corners
                android:bottomLeftRadius="7dp"
                android:bottomRightRadius="7dp"
                android:topLeftRadius="7dp"
                android:topRightRadius="7dp" />
        </shape>
    </item>
    <item android:left="3dp" android:right="3dp" android:bottom="3dp" android:top="3dp">
        <shape
            android:innerRadius="2px"
            android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="@color/black"/>
            <solid android:color="#88000000" />
            <corners
                android:bottomLeftRadius="7dp"
                android:bottomRightRadius="7dp"
                android:topLeftRadius="7dp"
                android:topRightRadius="7dp" />
        </shape>
    </item>
</layer-list>